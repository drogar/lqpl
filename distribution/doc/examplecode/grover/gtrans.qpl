#Import oracle.qpl
#Import hadList.qpl
#Import phase.qpl

gtrans::(dataqbs:List(Qubit);
         dataqbs:List(Qubit))=
{  hadList dataqbs;
   phase dataqbs;
   hadList dataqbs;
   oracle dataqbs;
}


doNGrovers::(n:Int | dataqbs:List(Qubit);
                     dataqbs:List(Qubit))=	
{  if (n==0) => {} //Done
   else      =>
      { gtrans dataqbs;
        doNGrovers(n-1) dataqbs}
}

  
   
